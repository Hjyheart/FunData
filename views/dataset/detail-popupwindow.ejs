<!--以下为点击new submition时弹出的页面-->
<div class="ui small modal" id="newSubmition">
    <i class="close icon"></i>
    <div class="header" id="detailName">
        New Submition
    </div>
    <div class="content">
        <form class="ui form">
            <div class="field">
                <label>Description</label>
                <input type="text" ng-model="desc" placeholder="pull request name..."/>
            </div>

            <div class="field">
                <label>Type</label>
                <select ng-model="_type" ng-change="select()" ng-options="t.text for t in types">

                </select>
            </div>

            <div class="field aligned" id="dataset-upload">
                <a href="#" id="upload">New File</a>
            </div>
            <div class="actions">
                <div ng-click="upload(desc)" class="ui positive right floated labeled icon button approve" type="submit">Yes <i class="checkmark icon"></i> </div>
            </div>
            <br>
        </form>
    </div>
</div>


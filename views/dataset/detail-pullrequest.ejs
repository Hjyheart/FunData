<!--这边打表用循环-->
<div class="ui bottom attached tab segment" data-tab="Two">
    <div class="ui items">
        <div class="item">
            <button class="orange ui button" onclick="newSubmition()">New Submition</button>
        </div>
        <hr>
        <div ng-repeat="pull in PullRequests">
            <div class="item">
                <div class="image">
                    <img src="http://semantic-ui.com/images/wireframe/image.png">
                </div>
                <div class="content">
                    <a class="header">{{pull.author}}</a>
                    <div class="meta">
                        <span class="cinema">desc</span>
                    </div>
                    <div class="description">
                        <p></p>
                    </div>
                    <div class="extra">
                        <div class="ui right floated primary button" onclick="showPullRequest('<%= title %>')">More <i class="right chevron icon"></i> </div>
                    </div>
                </div>
            </div>
            <hr>
        </div>

    </div>
</div>



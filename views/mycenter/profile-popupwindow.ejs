<!--以下为点击edit时弹出的页面-->
<div class="ui small modal" id="edit">
    <i class="close icon"></i>
    <div class="header" id="edit-title">
        Change Password
    </div>
    <div class="ui form">
        <div class="ui stacked segment">
            <div class="field" id="first-name">
                <label>Old Password</label>
                <div class="ui left icon input">
                    <i class="lock icon"></i>
                    <input placeholder="Old Password" type="password" ng-model="oldpwd">
                </div>
            </div>
            <div class="field" id="last-name">
                <label>New Password</label>
                <div class="ui left icon input">
                    <i class="lock icon"></i>
                    <input placeholder="New Password" type="password" ng-model="newpwd">
                </div>
            </div>
            <div class="actions">
                <button class="ui green submit button approve" ng-click="changePassword()">Ok</button>
            </div>
        </div>
    </div>
</div>

<!--以下为点击ChangeImg时弹出的页面-->
<div class="ui modal" id="new-image">
    <div class="header" id="upload-modual-header">Change Imge</div>
    <div class="content">
        <div class="ui form">
            <div class="field" id="dataset-upload">
                <a href="#" id="upload">upload</a>
            </div>
        </div>
    </div>
    <div class="actions">
        <button class="ui green submit button approve">Ok</button>
    </div>
</div>

<!--以下为createDataset时弹出的页面-->
<div class="ui modal" id="createDataset">
    <i class="close icon"></i>
    <div class="header">
        Add Dataset
    </div>
    <div class="ui form">
        <div class="ui stacked segment" id="container">
            <div class="field" id="dataset-name">
                <label>Dataset Name</label>
                <div class="ui input">
                    <input placeholder="DataSet Name" type="text" ng-model="name">
                </div>
            </div>
            <div class="field" id="dataset-desc">
                <label>Description</label>
                <div class="ui input">
                    <input placeholder="Description" type="text" ng-model="desc">
                </div>
            </div>
            <div class="actions">
                <button class="ui green submit button approve" ng-click="submitDataset(name, desc)">Ok</button>
            </div>
        </div>
    </div>
</div>

<!--以下为createCompetition时弹出的页面-->
<div class="ui modal" id="createCompetition">
    <i class="close icon"></i>
    <div class="header">
        Add Competition
    </div>
    <div class="ui form">
        <div class="ui stacked segment">
            <div class="field">
                <label>Competition Name</label>
                <div class="ui input">
                    <input placeholder="Competition Name" type="text" id="com-name">
                </div>
            </div>
            <div class="field">
                <label>Description</label>
                <div class="ui input">
                    <input placeholder="Description" type="text" id="com-des">
                </div>
            </div>
            <div class="field">
                <label>start time</label>
                <div class="ui input">
                    <input id="start-time" name="start-date" type="text" placeholder="start time">
                </div>
            </div>
            <div class="field">
                <label>end time</label>
                <div class="ui input">
                    <input id="end-time" name="start-date" type="text" placeholder="end time">
                </div>
            </div>
            <div class="actions">
                <button class="ui green submit button approve" ng-click="createCom()">Ok</button>
            </div>
        </div>
    </div>
</div>


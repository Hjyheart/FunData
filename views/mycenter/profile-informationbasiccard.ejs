<div class="ui cards">
    <div class="card">
        <div class="image">
            <img src="{{myprofile.head_href}}">
        </div>
        <div class="content">
            <div class="header">{{myprofile.user_name}}</div>
            <div class="meta">
                <a>{{myprofile.user_email}}</a>
            </div>
        </div>
        <div class="extra content">
            <span class="right floated">Joined in 2016 </span>
            <span><i class="user icon"></i> No.{{myprofile.user_id}}  </span>
        </div>
    </div>
</div>